{
  "apiVersion": 2,
  "name": "tmbp/team-members",
  "position": "Developer",
  "title": "Team Members",
  "category": "widgets",
  "icon": "groups",
  "description": "Display your team members in a grid layout.",
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "background": {
      "backgroundSize": true,
      "backgroundImage": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "color": {
      "heading": true,
      "text": true
    }
  },
  "textdomain": "team-members-block-plugin",
  "selectors": {
    "root": ".wp-block-tmbp-team-members"
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": [
    "file:./style-index.css",
    "file:./view.css"
  ],
  "render": "file:./render.php",
  "viewScript": "file:./view.js",
  "attributes": {
    "style": {
      "type": "object",
      "default": {
        "color": {
          "text": "green"
        }
      }
    },
    "template": {
      "type": "string",
      "default": "template1"
    },
    "members": {
      "type": "array",
      "default": [
        {
          "name": "John Doe",
          "position": "Developer",
          "role": "CEO & Founder",
          "image": "https://picsum.photos/200/300",
          "bio": "John has been leading our company for over a decade, bringing innovation and growth to every project.",
          "social": [
            {
              "link": "#",
              "icon": {
                "class": "fab fa-facebook-f"
              }
            },
            {
              "link": "#",
              "icon": {
                "class": "fab fa-twitter"
              }
            },
            {
              "link": "#",
              "icon": {
                "class": "fab fa-linkedin-in"
              }
            }
          ]
        }
      ]
    },
    "styles": {
      "type": "object",
      "default": {
        "template1": {},
        "common": {}
      }
    }
  }
}